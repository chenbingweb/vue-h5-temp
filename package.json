{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "serve:online": "vue-cli-service serve --mode devonline",
    "build": "vue-cli-service build",
    "build:prod": "vue-cli-service build --mode prod",
    "build:dev": "vue-cli-service build --mode staging",
    "watch": "vue-cli-service watch",
    "deploy:dev": "npm run build && cross-env NODE_ENV=dev node ./deploy",
    "deploy:prod": "npm run build && cross-env NODE_ENV=prod node ./deploy",
    "lint": "vue-cli-service lint"
  },
  "vuePlugins": {
    "service": [
      "./vue-cli-plugin-watch"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@sentry/browser": "^5.18.1",
    "@sentry/integrations": "^5.18.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "core-js": "^2.6.5",
    "echarts": "^5.3.2",
    "hls.js": "^0.13.2",
    "jquery": "^3.4.1",
    "lib-flexible": "^0.3.2",
    "postcss-px2rem": "^0.3.0",
    "qs": "^6.9.1",
    "vant": "^2.2.16",
    "vconsole": "^3.3.4",
    "vue": "^2.6.10",
    "vue-ant-util": "^1.0.1",
    "vue-aplayer": "^1.6.1",
    "vue-file-upload": "^0.1.12",
    "vue-router": "^3.1.3",
    "vue-video-player": "^5.0.2",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-es2015": "^6.24.1",
    "co-wechat": "^2.4.0",
    "co-wechat-oauth": "^2.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "scp2": "^0.5.0",
    "velocity-animate": "^1.5.2",
    "vue-template-compiler": "^2.6.10",
    "wechat-api": "^1.35.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jquery": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
